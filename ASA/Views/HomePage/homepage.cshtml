<!DOCTYPE html>
<html>

<head>
	<title>AUST STUDENT ASSISTANCE</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="~/Content/HomeStyle.css" rel="stylesheet" type="text/css" >
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css">

	<!-- <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous"> -->

	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
	<!--<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script> -->


</head>

<body>


			<div class="header">

				<a href="#" id="menu-toggle"><span class="glyphicon glyphicon-menu-hamburger"></span></a>

			</div>
       

			<!--SIDE BAR MENU Start-->

	<div id="wrapper">


		<div id="sidebar-wrapper">
			<ul class="sidebar-nav">
				<li><a href="homepage"> <span class="glyphicon glyphicon-home"></span> Home</a></li>
				<li><a href="contact"> <span class="glyphicon glyphicon-phone-alt"></span> Contact</a></li>
				<li><a href="#"> <span class="glyphicon glyphicon-user"></span> About</a></li>
				<li class="settings"><a href="#"> <span class="glyphicon glyphicon-cog"></span> Settings</a></li>

			</ul>
		</div>

		<script>
			$("#menu-toggle").click(function (e) {
				e.preventDefault();
				$("#wrapper").toggleClass("menuDisplayed");
			});
		</script>


		<!--WEB NAME LOGO-->

		<div class="webname">

			<a href="homepage"><img src="~/Content/images/logo.jpg"></a>


		</div>

		<!--SEARCH BAR-->

		<div class="search">

			<form>

				<div class="input-group">
					<input type="text" name="search" class="form-control" placeholder="Search">
					<div class="input-group-btn">
						<button class="btn btn-danger" type="submit"> <i class="glyphicon glyphicon-search"></i></button>

					</div>

				</div>
			</form>
		</div>

		<!-- FOR SWATCHING MODAL -->
		<script type="text/javascript">
			$(document).ready(function () {



				$("#gotosignup").click(function () {
					$("#loginModal").modal('hide');
					$("#signupModal").modal('show');
				});


				$("#gotologin").click(function () {
					$("#signupModal").modal('hide');
					$("#loginModal").modal('show');
				});

			});
		</script>

		<!-- LOG IN MODAL START -->

		<div class="loginbox">

			<button id="btnlogin" class="btn btn-danger" type="button" data-toggle="modal" data-target="#loginModal"><span class="glyphicon glyphicon-log-in"></span> LogIn</button>



			<!-- Modal -->
			<div id="loginModal" class="modal fade" role="dialog">
				<div class="modal-dialog">

					<!-- Modal content-->
					<div class="modal-content">
						<div class="modal-header">
							<button type="button" class="close" data-dismiss="modal">&times;</button>
							<h4 class="modal-title">Log In To Pickachoo to Shop Harder!!</h4>
						</div>
						<div class="modal-body">

							<form id="login-box" method="post" action="">
								<div class="log">



									<input type="text" name="email" placeholder="E-mail" /> <br>
									<input type="password" name="password" placeholder="Password" /> <br>

									<div class="checkbox">
										<label><input type="checkbox" name="remember"> Remember me</label>
									</div>
									<input type="submit" class="btn btn-danger" name="submit" value="Login" />

									<h1>New to Pickachoo? Then...</h1>
									<button id="gotosignup" type="button" class="btn btn-danger">Signup</button>

								</div>

							</form>



						</div>
						<div class="modal-footer">
							<button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
						</div>
					</div>

				</div>
			</div>
		</div>

		<!-- SIGN UP MODAL START -->

		<div class="registerbox">

			<button id="btnsignup" class="btn btn-danger" type="submit" data-toggle="modal" data-target="#signupModal"><span class="glyphicon glyphicon-user"></span>Register</button>

			<div id="signupModal" class="modal fade" role="dialog">
				<div class="modal-dialog">

					<!-- Modal content-->
					<div class="modal-content">
						<div class="modal-header">
							<button type="button" class="close" data-dismiss="modal">&times;</button>
							<h4 class="modal-title">SignUp For Pickachoo</h4>
						</div>
						<div class="modal-body">

							<form id="signup-box" method="post" action="">
								<div class="signup">
									<h1>Sign up</h1>

									<input type="text" name="name" placeholder="Name" />
									<input type="text" name="email" placeholder="E-mail" />
									<input type="password" name="password" placeholder="Password" />
									<input type="text" name="phone" placeholder="Phone" />

									<input type="submit" class="btn btn-danger" name="submit" value="SIGN ME UP" />
									<p>Already Have an Account?</p>


									<button id="gotologin" type="button" class="btn btn-danger">LogIn</button>
								</div>

							</form>

						</div>

						<div class="modal-footer">
							<button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
						</div>

					</div>
				</div>

			</div>
		</div>



		<!--Slider start-->
		<div id="page-content-wrapper">

			<div class="container">

				<script type="text/javascript">
					$(document).ready(function () {
						$('.carousel').carousel({
							interval: 3000
						})
					});
				</script>

				<div id="slider" class="carousel slide">

					<ol class="carousel-indicators">

						<li data-target="#slider" data-slide-to="0" class="active"></li>
						<li data-target="#slider" data-slide-to="1" class=""></li>
						<li data-target="#slider" data-slide-to="2" class=""></li>
					</ol>



					<div class="carousel-inner">

						<div class="item active">
							<img src="~/Content/images/7.jpg">

						</div>


						<div class="item ">
							<img src="~/Content/images/8.jpg">

						</div>

						<div class="item">
							<img src="~/Content/images/7.jpg">

						</div>

					</div>

					<a class="left carousel-control" href="#slider" data-slide="prev">

						<span class="glyphicon glyphicon-chevron-left"></span>
						<span class="sr-only">Previous</span>
					</a>

					<a class="right carousel-control" href="#slider" data-slide="next">

						<span class="glyphicon glyphicon-chevron-right"></span>
						<span class="sr-only">Next</span>
					</a>



				</div>
			</div>
		</div>



		<div id="page-content-wrapper">
			<div class="mainbody">

				



			</div>
		</div>

	</div>

			<!--FOOTER START -->

			<div class="footer">

				<footer></footer>

			</div>

</body>

</html>




